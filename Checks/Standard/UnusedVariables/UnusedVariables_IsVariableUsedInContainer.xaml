          <ui:ForEach x:TypeArguments="sx:XmlNode" DisplayName="For each attribute" sap2010:WorkflowViewState.IdRef="ForEach`1_8" Values="[AttributeNodes]">
            <ui:ForEach.Body>
              <ActivityAction x:TypeArguments="sx:XmlNode">
