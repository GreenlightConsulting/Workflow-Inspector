  <Sequence sap2010:Annotation.AnnotationText="The top level activity (usually a flowchart or sequence) of a workflow should have a brief textual description giving an overview of its purpose. This helps developers to quickly understand the workflow's goal, inputs, outputs, preconditions and outcomes.&#xA;&#xA;The steps to find the workflow annotation are as follows:&#xA;1 - Get the first child of the node of the root element that is an activity&#xA;2 - Check whether that node has an annotation by looking for the attribute Annotation.AnnotationText" DisplayName="Check for missing workflow annotation" sap2010:WorkflowViewState.IdRef="Sequence_3">
    <Sequence.Variables>
      <Variable x:TypeArguments="sx:XmlNode" Name="TopLevelActivity" />
      <Variable x:TypeArguments="x:String" Name="XPathExpression" />
      <Variable x:TypeArguments="x:String" Name="InternalPath" />
    </Sequence.Variables>
    <Assign sap2010:Annotation.AnnotationText="Retrieves the first activity that is a child of Activity (the root element)." sap2010:WorkflowViewState.IdRef="Assign_2">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[XPathExpression]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">["/xaml:Activity/xaml:Sequence | /xaml:Activity/xaml:Flowchart | /xaml:Activity/xaml:StateMachine | /xaml:Activity/*[@sap2010:WorkflowViewState.IdRef]"]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="sx:XmlNode">[TopLevelActivity]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sx:XmlNode">[in_XMLTree.DocumentElement.SelectSingleNode(XPathExpression, in_NamespaceManager)]</InArgument>
      </Assign.Value>
    </Assign>
    <If Condition="[TopLevelActivity.Attributes(&quot;Annotation.AnnotationText&quot;) Is Nothing]" sap2010:WorkflowViewState.IdRef="If_3">
