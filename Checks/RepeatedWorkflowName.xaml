  <Sequence sap2010:Annotation.AnnotationText="Workflows in a project should have unique names (even if they are in different folders) to facilitate project organization and debugging.&#xA;&#xA;The steps to find a repeated workflow name are as follows:&#xA;1 - Retrieve all workflow paths&#xA;2 - Checks whether the workflow filename has been found previously" DisplayName="Check for repeated workflow name" sap2010:WorkflowViewState.IdRef="Sequence_3">
    <Sequence.Variables>
      <Variable x:TypeArguments="scg:Dictionary(x:String, scg:List(x:String))" Default="[New Dictionary(Of String, List(Of String))]" Name="WorkflowsNamesIndex" />
      <Variable x:TypeArguments="scg:List(x:String)" Default="[New List(Of String)]" Name="RepeatedWorkflowsPaths" />
    </Sequence.Variables>
    <ui:ForEach x:TypeArguments="x:String" DisplayName="For each workflow in project" sap2010:WorkflowViewState.IdRef="ForEach`1_2" Values="[Directory.GetFiles(in_ProjectPath, &quot;*.xaml&quot;, SearchOption.AllDirectories)]">
      <ui:ForEach.Body>
        <ActivityAction x:TypeArguments="x:String">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="x:String" Name="WorkflowFilepath" />
          </ActivityAction.Argument>
          <If sap2010:Annotation.AnnotationText="Creates an index of workflow name and its paths (if repeated).&#xA;It considers only the name of the file and its extension, not the path." Condition="[WorkflowsNamesIndex.ContainsKey(Path.GetFileName(WorkflowFilepath))]" DisplayName="If workflow name was previously found" sap2010:WorkflowViewState.IdRef="If_3">
            <If.Then>
              <AddToCollection x:TypeArguments="x:String" Collection="[WorkflowsNamesIndex(Path.GetFileName(WorkflowFilepath))]" DisplayName="Add path of workflow with repeated name" sap2010:WorkflowViewState.IdRef="AddToCollection`1_2" Item="[WorkflowFilepath]" />
            </If.Then>
            <If.Else>
              <Assign sap2010:WorkflowViewState.IdRef="Assign_6">
                <Assign.To>
                  <OutArgument x:TypeArguments="scg:List(x:String)">[WorkflowsNamesIndex(Path.GetFileName(WorkflowFilepath))]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="scg:List(x:String)">[New List(Of String) From {WorkflowFilepath}]</InArgument>
                </Assign.Value>
              </Assign>
            </If.Else>
          </If>
        </ActivityAction>
      </ui:ForEach.Body>
    </ui:ForEach>
    <ui:ForEach x:TypeArguments="scg:KeyValuePair(x:String, scg:List(x:String))" sap2010:Annotation.AnnotationText="Verifies what workflows have repeated names and adds to a list that will be output to the report." DisplayName="For each workflow name and its paths" sap2010:WorkflowViewState.IdRef="ForEach`1_4" Values="[WorkflowsNamesIndex]">
      <ui:ForEach.Body>
        <ActivityAction x:TypeArguments="scg:KeyValuePair(x:String, scg:List(x:String))">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="scg:KeyValuePair(x:String, scg:List(x:String))" Name="WorkflowNameAndPaths" />
          </ActivityAction.Argument>
          <If sap2010:Annotation.AnnotationText="Checks whether a workflow name was repeated." Condition="[WorkflowNameAndPaths.Value.Count &gt; 1]" sap2010:WorkflowViewState.IdRef="If_5">
            <If.Then>
              <AddToCollection x:TypeArguments="x:String" Collection="[RepeatedWorkflowsPaths]" DisplayName="Add to RepeatedWorkflowsPaths" sap2010:WorkflowViewState.IdRef="AddToCollection`1_4" Item="[String.Join(&quot;&lt;-&gt;&quot;, WorkflowNameAndPaths.Value)]" />
            </If.Then>
          </If>
        </ActivityAction>
      </ui:ForEach.Body>
    </ui:ForEach>
    <Assign sap2010:Annotation.AnnotationText="Appends all repeated paths and returns it to be output to the report.&#xD;&#xA;The brackets are appended for more legibility in the report." DisplayName="Assign out_OptionalMessage" sap2010:WorkflowViewState.IdRef="Assign_8">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[out_OptionalMessage]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[": [" + String.Join(", ", RepeatedWorkflowsPaths) + "]"]</InArgument>
      </Assign.Value>
    </Assign>
    <If Condition="[String.IsNullOrWhiteSpace(out_OptionalMessage)]" DisplayName="If repeated names were found and included in the optional message" sap2010:WorkflowViewState.IdRef="If_4">
      <If.Then>
        <Assign DisplayName="Assign out_HasProjectIssue (False)" sap2010:WorkflowViewState.IdRef="Assign_5">
